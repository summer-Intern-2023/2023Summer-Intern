    "import re\n",
    "\n",
    "filename = \"regex_sum_1840726.txt\"\n",
    "try:\n",
    "    file = open(filename)\n",
    "except:\n",
    "    print(\"Error to open the file\", filename)\n",
    "\n",
    "sum = 0\n",
    "for line in file :\n",
    "    linenums = re.findall('[0-9]+', line)\n",
    "    for linenum in linenums:\n",
    "        sum += int(linenum)\n",
    "\n",
    "print(sum)"
   ]
  }
